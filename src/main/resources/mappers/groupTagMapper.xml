<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.inhatc.mapper.GroupTagMapper">

	<insert id="create">
		insert 
			groupTag
		set 
			groupSeq = #{groupSeq}
			, groupTagName = #{groupTagName}
	</insert>
	
	<select id="readAll" resultType="com.inhatc.domain.GroupTagVO">
		select 
			* 
		from 
			groupTag 
		where 
			groupSeq = #{groupSeq}
	</select>
	
	<delete id="delete">
		delete from
			groupTag  
		where 
			groupTagSeq = #{groupTagSeq}
	</delete>
	
</mapper>