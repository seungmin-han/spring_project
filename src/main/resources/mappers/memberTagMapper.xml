<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.inhatc.mapper.MemberTagMapper">

	<insert id="create">
		insert 
			memberTag 
		set 
			memberSeq = #{memberSeq}
			, memberTagName = #{memberTagName}
	</insert>
	
	<select id="readAll" resultType="com.inhatc.domain.MemberTagVO">
		select 
			* 
		from 
			memberTag 
		where 
			memberSeq = #{memberSeq}
	</select>
	
	<delete id="delete">
		delete from
			memberTag  
		where 
			memberTagSeq = #{memberTagSeq}
	</delete>
	
	
</mapper>